<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:main="org.bigbluebutton.air.main.views.*"
		 width="100%"
		 height="100%">
	<fx:Script>
		<![CDATA[
			import org.bigbluebutton.air.common.utils.PagesENUM;
			
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:layout>
		<s:HorizontalLayout gap="0" />
	</s:layout>
	<main:NavigationButton id="deskshareBtn0"
						 navigateTo="{[PagesENUM.DESKSHARE]}"
						 width="100%"
						 height="100%"
						 visible="false"
						 includeInLayout="false"
						 styleName="deskshareBtnStyle" />
	<main:NavigationButton id="presentationBtn0"
						 width="100%"
						 height="100%"
						 navigateTo="{[PagesENUM.PRESENTATION]}"
						 styleName="presentationBtnStyle" />
	<main:NavigationButton id="videochatBtn0"
						 navigateTo="{[PagesENUM.VIDEO_CHAT]}"
						 width="100%"
						 height="100%"
						 styleName="videochatBtnStyle" />
	<main:NavigationButton id="chatBtn0"
						 navigateTo="{[PagesENUM.CHATROOMS,PagesENUM.CHAT,PagesENUM.SELECT_PARTICIPANT]}"
						 width="100%"
						 height="100%"
						 styleName="chatBtnStyle" />
	<main:NavigationButton id="participantsBtn0"
						 navigateTo="{[PagesENUM.PARTICIPANTS,PagesENUM.USER_DETAILS]}"
						 width="100%"
						 height="100%"
						 styleName="participantsBtnStyle" />
</s:Group>
